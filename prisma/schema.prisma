generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Formulario {
  id         String @id @default(uuid())
  pacienteId String
  medicoId   String
  tipo       String
  respuestas Json
}

model Norton {
  id             String   @id @default(uuid())
  pacienteId     String
  medicoId       String
  preguntaUno    String
  preguntaDos    String
  preguntaTres   String
  preguntaCuatro String
  preguntaCinco  String
  preguntaSeis   String
  preguntaSiete  String
  createdAt      DateTime @default(now())
}
