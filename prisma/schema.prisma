generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Formulario {
  id         String   @id @default(uuid())
  pacienteId String
  medicoId   String
  tipo       String
  respuestas Json
  createdAt  DateTime @default(now())
}
